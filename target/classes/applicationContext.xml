<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="communication" class="com.oraclejava.service.Communication">
<!--        <constructor-arg name="message" ref="activeMQMessage" />-->
<!--        <constructor-arg name="message" ref="kakaoMessage" />-->
        <property name="message" ref="activeMQMessage" />
    </bean>

    <bean id="activeMQMessage" class="com.oraclejava.message.ActiveMQMessage" />
    <bean id="kakaoMessage" class="com.oraclejava.message.KakaoMessage" />
</beans>